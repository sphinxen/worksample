<%= form_for @supplier do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name%>
	<%= f.label :phone %>
	<%= f.text_field :phone%>
	<%= f.label :email %>
	<%= f.text_field :email%>
	<%= f.label :category %>
	<%= f.text_field :category%>
	<%= f.label :address %>
	<%= f.text_field :address%>
	<%= f.label :city %>
	<%= f.text_field :city%>
	<%= f.submit %>
<% end%>